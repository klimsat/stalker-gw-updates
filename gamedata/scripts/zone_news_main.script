function send_tip(news_id, icon)
  if news_id == nil then
    return
  end

  xr_sound.set_sound_play(db.actor:id(), "pda_tips")
  local news_caption = GetString("st_tip")
  utils.news_params(news_caption, news_id, icon or "ui_iconsTotal_grouping", 0, 10000)
end

function news_main()
  local item = sms[math.random(utils.get_table_length(sms))]

  if item == nil then
    return
  end

  local is_faction = utils.table_has_item(sim_tables.factions, item.key)

  if
    is_faction and sim_factions.faction_information[item.key] and sim_factions.faction_information[item.key].passive_npc and
      sim_factions.faction_information[item.key].passive_npc == 0 and
      sim_factions.faction_information[item.key].active_npc and
      sim_factions.faction_information[item.key].active_npc == 0
   then
    return
  end

  send_tip(item.messages[math.random(#item.messages)], item.icon)
end

