[plugin]
enable = true
refused_names = none
in_combat = true					;Can use in combat
out_combat = true					;Can use outside combat
medkit_health = 60				;Percent of health before npc uses medkit
bandage_bleeding = 0.1
medkits = stimpack,stimpack_army,stimpack_scientic,medkit,medkit_army,medkit_scientic		; Usable sections
bandages = stimpack,stimpack_army,stimpack_scientic,bandage
excluded_communities = zombied

[novice]
medkit_delay = 1240,1460				;Min-Max delay before eating
bandage_delay = 1240,1460

[experienced]
medkit_delay = 1020,1090				;Min-Max delay before eating
bandage_delay = 1020,1090

[veteran]
medkit_delay = 880,940				;Min-Max delay before eating
bandage_delay = 880,940

[master]
medkit_delay = 710,860				;Min-Max delay before eating
bandage_delay = 710,860

